<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace="amdn.anywhere.mapper.BadWordsMapper">
<resultMap type="BadWords" id="BadWordsMap">
	<result property="badWordsCode"		column="bad_words_code"></result>
	<result property="banWordsList"		column="ban_words_list"></result>
</resultMap>


<!-- 금지어 리스트 조회 -->
<select id="getBadWordsList" resultMap="BadWordsMap">
	SELECT
		bad_words_code
		,ban_words_list
	FROM
		tb_bad_words;
</select>
</mapper>