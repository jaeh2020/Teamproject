<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="amdn.anywhere.mapper.SalesMapper">
<resultMap type="Sales" id="salesResultMap">
	<result property="storeCode" column="s_code"/>
	<result property="salesName" column="s_name"/>
	<result property="salesPrice" column="s_price"/>
	<result property="salesRegTime" column="s_regtime"/>
	<result property="closeTime" column="c_time"/>
</resultMap>		




<select id="getSalesList" resultMap="salesResultMap" parameterType="String">
	SELECT
		*
	FROM
		tb_account_order_d		AS S
		LEFT JOIN
		tb_expense_d 				AS E
	ON	
		E.store_code=S.store_code
</select>

<insert id="addSales" parameterType="sales">
	INSERT INTO tb_question


</insert>

</mapper>